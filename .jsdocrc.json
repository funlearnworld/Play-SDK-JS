{
  "opts": {
    "template": "node_modules/@leeyeh/jsdoc-rtd",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["./node_modules/jsdoc-export-default-interop/dist/index"]
}
